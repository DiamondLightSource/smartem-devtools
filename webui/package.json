{
  "name": "smartem-dev-webui",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "prebuild": "npx tsx scripts/prebuild.ts",
    "dev": "npm run prebuild && vite",
    "build": "npm run prebuild && tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "router:generate": "tsr generate",
    "router:check": "tsr check",
    "lint": "biome lint ./src",
    "lint:fix": "biome lint --write ./src",
    "format": "biome format --write ./src && prettier --write './**/*.{yml,yaml,md}'",
    "format:biome": "biome format --write ./src",
    "format:prettier": "prettier --write './**/*.{yml,yaml,md}'",
    "format:check": "biome format ./src && prettier --check './**/*.{yml,yaml,md}'",
    "check": "biome check ./src",
    "check:fix": "biome check --write ./src"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mdx-js/react": "^3.1.1",
    "@mdx-js/rollup": "^3.1.1",
    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.0.2",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-router": "^1.159.3",
    "@tanstack/router-devtools": "^1.159.3",
    "@tanstack/router-vite-plugin": "^1.159.3",
    "@vitejs/plugin-react": "^5.1.3",
    "minisearch": "^7.2.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "rehype-pretty-code": "^0.14.1",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "shiki": "^3.22.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.14",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/router-cli": "^1.158.4",
    "@types/node": "^25.2.2",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.0.1",
    "lefthook": "^2.1.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.2",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.1.0"
  }
}
